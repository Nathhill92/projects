stages:
  - prep
  - build
  - test

prep the test:
  stage: prep
  script:
    - mkdir build
    - cd build
    - touch car.txt

build the car:
  stage: build
  script:
    - echo "chassis" >> car.txt
    - echo "engine" >> car.txt
    - echo "wheels" >> car.txt
  artifacts:
    paths:
      - build/
    
test the car:
  stage: test
  script:
    - test -f build/car.txt
    - cd build
    - grep "chassis" car.txt
    - grep "engine" car.txt
    - grep "wheels" car.txt
